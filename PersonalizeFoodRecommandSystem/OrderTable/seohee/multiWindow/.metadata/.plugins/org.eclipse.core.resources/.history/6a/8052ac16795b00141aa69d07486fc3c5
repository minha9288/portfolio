	public InternalFrame4() {
		super("table4", false, false, false, false);
		setLayout(new BorderLayout());
		setSize(Toolkit.getDefaultToolkit().getScreenSize().width/2,Toolkit.getDefaultToolkit().getScreenSize().height/2);
